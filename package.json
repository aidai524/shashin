{
  "name": "gemini-proxy",
  "version": "1.0.0",
  "description": "Cloudflare Workers proxy for Google Gemini API",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:frontend": "wrangler pages deploy frontend --project-name=gemini-image-generator",
    "deploy:miniapp": "wrangler deploy --config wrangler-miniapp.toml",
    "dev:miniapp": "wrangler dev --config wrangler-miniapp.toml",
    "tail": "wrangler tail",
    "tail:miniapp": "wrangler tail --config wrangler-miniapp.toml",
    "serve": "npx serve frontend -l 8080",
    "frontend": "npx serve frontend -l 8080"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "gemini",
    "proxy",
    "api",
    "miniapp"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "wrangler": "^4.54.0"
  }
}
